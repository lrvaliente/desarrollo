pipeline {
	agent { label 'master' }
		stages{
			stage('Iniciar'){
				steps {
					sh "mvn --version";
				}
			}
			stage('test'){
				steps {
					sh "mvn clean compile test";
				}
			}
			
			stage ('Build application') {
				steps {
					echo "mvn clean install -Dmaven.test.skip-true"
				}
			}
			
			stage ('Other') {
				steps {
					echo "other..."
				}
			}
		}
}